   <children>
      <Label fx:id="lblMensagem" layoutX="44.0" layoutY="48.0" text="Vai aparecer uma Mensagem" textAlignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="btnClick" layoutX="111.0" layoutY="100.0" mnemonicParsing="false" onAction="#clicouBotao" text="Clique Aqui" textFill="RED">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
